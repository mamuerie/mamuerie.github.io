@article{vandierendonck2017,
  title = {A Comparison of Methods to Combine Speed and Accuracy Measures of Performance: {{A}} Rejoinder on the Binning Procedure},
  author = {Vandierendonck, André},
  date = {2017-01-01T00:00:00},
  journaltitle = {Behavior research methods},
  shortjournal = {Behav Res Methods},
  volume = {49},
