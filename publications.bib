@article{boksem2008,
  title = {Mental Fatigue: Costs and Benefits},
  author = {Boksem, Maarten A. S. and Tops, Mattie},
  date = {2008-01-01T00:00:00},
  journaltitle = {Brain research reviews},
  shortjournal = {Brain Res Rev},
  volume = {59},
