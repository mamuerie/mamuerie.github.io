}

@article{gargouri2018,
  title = {The {{Influence}} of {{Preprocessing Steps}} on {{Graph Theory Measures Derived}} from {{Resting State fMRI}}},
  author = {Gargouri, Fatma and Kallel, Fathi and Delphine, Sebastien and Ben Hamida, Ahmed and Lehéricy, Stéphane and Valabregue, Romain},
