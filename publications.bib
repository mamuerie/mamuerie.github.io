@article{mifsud2016,
  title = {Self-Initiated Actions Result in Suppressed Auditory but Amplified Visual Evoked Components in Healthy Participants},
  author = {Mifsud, Nathan G. and Oestreich, Lena K. L. and Jack, Bradley N. and Ford, Judith M. and Roach, Brian J. and Mathalon, Daniel H. and Whitford, Thomas J.},
  date = {2016-01-01T00:00:00},
  journaltitle = {Psychophysiology},
  shortjournal = {Psychophysiology},
  volume = {53},
