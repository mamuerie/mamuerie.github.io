@article{hazeltine2003,
  title = {Material-Dependent and Material-Independent Selection Processes in the Frontal and Parietal Lobes: An Event-Related {{fMRI}} Investigation of Response Competition},
  shorttitle = {Material-Dependent and Material-Independent Selection Processes in the Frontal and Parietal Lobes},
