  keywords = {Adult,Aging,Alpha Rhythm,Cerebral Cortex/physiology/physiopathology,Female,Frequency analysis,Humans,Male,Mental fatigue,Mental Fatigue/physiopathology,Middle Aged,Theta,Theta Rhythm/physiology,Young Adult},
