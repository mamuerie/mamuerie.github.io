@article{rey-mermet2019,
  title = {Is Executive Control Related to Working Memory Capacity and Fluid Intelligence?},
  author = {Rey-Mermet, Alodie and Gade, Miriam and Souza, Alessandra S. and Bastian, Claudia C. and Oberauer, Klaus},
  date = {2019-01-01T00:00:00},
