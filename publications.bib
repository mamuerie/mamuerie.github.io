  author = {Faskowitz, Joshua and Esfahlani, Farnaz Zamani and Jo, Youngheun and Sporns, Olaf and Betzel, Richard F.},
  date = {2020-01-01T00:00:00},
  journaltitle = {Nature neuroscience},
