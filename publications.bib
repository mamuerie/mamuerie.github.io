@article{moisala2015,
  title = {Brain Activity during Divided and Selective Attention to Auditory and Visual Sentence Comprehension Tasks},
  author = {Moisala, Mona and Salmela, Viljami and Salo, Emma and Carlson, Synnöve and Vuontela, Virve and Salonen, Oili and Alho, Kimmo},
  date = {2015-01-01T00:00:00},
