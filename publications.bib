}

@article{stelzel2011a,
  title = {Interference Effects of Stimulus-Response Modality Pairings in Dual Tasks and Their Robustness},
  author = {Stelzel, Christine and Schubert, Torsten},
  date = {2011-01-01T00:00:00},
  journaltitle = {Psychological research},
