@article{janczyk2014,
  title = {Who Is Talking in Backward Crosstalk? {{Disentangling}} Response- from Goal-Conflict in Dual-Task Performance},
  author = {Janczyk, Markus and Pfister, Roland and Hommel, Bernhard and Kunde, Wilfried},
