@article{hilger2017,
  title = {Efficient Hubs in the Intelligent Brain: {{Nodal}} Efficiency of Hub Regions in the Salience Network Is Associated with General Intelligence},
  shorttitle = {Efficient Hubs in the Intelligent Brain},
  author = {Hilger, Kirsten and Ekman, Matthias and Fiebach, Christian J. and Basten, Ulrike},
