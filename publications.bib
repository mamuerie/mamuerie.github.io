@article{smith2019,
  title = {Comparing the {{Effects}} of {{Three Cognitive Tasks}} on {{Indicators}} of {{Mental Fatigue}}},
  author = {Smith, Mitchell R. and Chai, Rifai and Nguyen, Hung T. and Marcora, Samuele M. and Coutts, Aaron J.},
