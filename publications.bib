@article{mochizuki2007,
  title = {Brain Activity Associated with Dual-Task Management Differs Depending on the Combinations of Response Modalities},
  author = {Mochizuki, Hideki and Tashiro, Manabu and Gyoba, Jiro and Suzuki, Miho and Okamura, Nobuyuki and Itoh, Masatoshi and Yanai, Kazuhiko},
