@article{braem2019,
  title = {Measuring {{Adaptive Control}} in {{Conflict Tasks}}},
  author = {Braem, Senne and Bugg, Julie M. and Schmidt, James R. and Crump, Matthew J.C. and Weissman, Daniel H. and Notebaert, Wim and Egner, Tobias},
  date = {2019-09},
  journaltitle = {Trends in cognitive sciences},
