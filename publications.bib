@article{weissenbacher2009,
  title = {Correlations and Anticorrelations in Resting-State Functional Connectivity {{MRI}}: A Quantitative Comparison of Preprocessing Strategies},
  author = {Weissenbacher, Andreas and Kasess, Christian and Gerstl, Florian and Lanzenberger, Rupert and Moser, Ewald and Windischberger, Christian},
  date = {2009-01-01T00:00:00},
  journaltitle = {NeuroImage},
  shortjournal = {Neuroimage},
  volume = {47},
