@article{perri2014,
  title = {Individual Differences in Response Speed and Accuracy Are Associated to Specific Brain Activities of Two Interacting Systems},
  author = {Perri, Rinaldo Livio and Berchicci, Marika and Spinelli, Donatella and Di Russo, Francesco},
  date = {2014-01-01T00:00:00},
  journaltitle = {Frontiers in behavioral neuroscience},
