@article{waller2018,
  title = {{{GraphVar}} 2.0: {{A}} User-Friendly Toolbox for Machine Learning on Functional Connectivity Measures},
  author = {Waller, L. and Brovkin, A. and Dorfschmidt, L. and Bzdok, D. and Walter, H. and Kruschwitz, J. D.},
