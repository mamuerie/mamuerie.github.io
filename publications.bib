@article{altmann2008,
  title = {An Integrated Model of Cognitive Control in Task Switching},
  author = {Altmann, Erik M. and Gray, Wayne D.},
  date = {2008-01-01T00:00:00},
  journaltitle = {Psychological review},
  shortjournal = {Psychol Rev},
  volume = {115},
