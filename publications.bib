}

@article{deprez2013,
  title = {The Functional Neuroanatomy of Multitasking: Combining Dual Tasking with a Short Term Memory Task},
  author = {Deprez, Sabine and Vandenbulcke, Mathieu and Peeters, Ron and Emsell, Louise and Amant, Frederic and Sunaert, Stefan},
  date = {2013-01-01T00:00:00},
  journaltitle = {Neuropsychologia},
