@article{mcfarquhar2019,
  title = {Modeling {{Group-Level Repeated Measurements}} of {{Neuroimaging Data Using}} the {{Univariate General Linear Model}}},
  author = {McFarquhar, Martyn},
  date = {2019-01-01T00:00:00},
