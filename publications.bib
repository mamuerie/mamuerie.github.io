@article{dux2009,
  title = {Training Improves Multitasking Performance by Increasing the Speed of Information Processing in Human Prefrontal Cortex},
  author = {Dux, Paul E. and Tombu, Michael N. and Harrison, Stephenie and Rogers, Baxter P. and Tong, Frank and Marois, René},
