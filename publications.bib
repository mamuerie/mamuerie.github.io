@article{delorme2012,
  title = {Independent {{EEG}} Sources Are Dipolar},
  author = {Delorme, Arnaud and Palmer, Jason and Onton, Julie and Oostenveld, Robert and Makeig, Scott},
  date = {2012-01-01T00:00:00},
