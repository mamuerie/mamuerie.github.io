}

@article{mockel2015,
  title = {The {{Effects}} of {{Time}} on {{Task}} in {{Response Selection--An ERP Study}} of {{Mental Fatigue}}},
  author = {Möckel, Tina and Beste, Christian and Wascher, Edmund},
  date = {2015-01-01T00:00:00},
