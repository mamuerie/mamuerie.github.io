 issn = {0031-5117, 1532-5962},
 journaltitle = {Perception & Psychophysics},
 langid = {english},
 number = {3},
 pages = {427--437},
 shortjournal = {Perception & Psychophysics},
 shorttitle = {Mapping Symbols to Response Modalities},
 title = {Mapping Symbols to Response Modalities: Interference Effects on Stroop-like Tasks},
 url = {http://link.springer.com/10.3758/BF03206864},
 urldate = {2024-08-07},
 volume = {60}
}
